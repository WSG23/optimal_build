{
  "name": "building-compliance-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preview:ci": "vite preview --host 127.0.0.1 --port 4173",
    "typecheck": "tsc --noEmit",
    "lint:tokens": "node ../scripts/enforce-feasibility-tokens.mjs",
    "lint:ui-standards": "node ../scripts/lint-ui-standards.mjs",
    "lint": "eslint --ext .ts,.tsx src",
    "test:smoke": "playwright test tests/e2e --reporter=list",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:capture": "vitest run -c vitest.capture.config.ts --coverage",
    "test:e2e": "node scripts/run-e2e.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.151",
    "@mui/material": "^5.14.17",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.17.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^2.9.0",
    "three": "^0.160.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/jsdom": "^27.0.0",
    "@types/mapbox-gl": "^3.0.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.8",
    "c8": "^9.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^24.1.3",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.15"
  }
