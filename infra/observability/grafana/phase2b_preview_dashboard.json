        {
          "datasource": {
            "type": "prometheus",
            "uid": "${prom_ds}"
          },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(preview_generation_job_duration_ms_bucket[15m])))",
          "legendFormat": "p95 duration",
          "refId": "B"
        }
      ],
      "title": "Preview job duration (rolling 15m)",
