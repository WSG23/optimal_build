[run]
branch = True
source =
    app
omit =
    app/api/v1/developers.py
    app/api/v1/finance.py
    app/models/business_performance.py
    app/models/listing_integration.py
    app/models/rkp.py
    app/schemas/finance.py
    app/services/developer_checklist_service.py
    app/services/developer_condition_service.py
    app/services/deals/performance.py
    app/services/entitlements.py
include =
    app/services/*
    app/utils/*

[paths]
app =
    backend/app
    app

[report]
fail_under = 80
show_missing = True
skip_covered = True
