# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=building_compliance
BUILDABLE_USE_POSTGIS=true
SLOW_QUERY_THRESHOLD_SECONDS=0.5

# Security
SECRET_KEY=your-secret-key-here
API_RATE_LIMIT=10/minute
ENVIRONMENT=development

# CORS
# Comma separated list or JSON array of origins allowed to call the backend API.
BACKEND_ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:5173","http://localhost:4400"]

# Services
REDIS_URL=redis://localhost:6379
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin

# CAD SDK Licenses
# Provide vendor-issued license keys via local secrets or deployment-time injection.
ODA_LICENSE_KEY=

# Task Processing
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1
RQ_REDIS_URL=redis://localhost:6379/2

# Overlay Job Queues
OVERLAY_QUEUE_LOW=overlay:low
OVERLAY_QUEUE_DEFAULT=overlay:default
OVERLAY_QUEUE_HIGH=overlay:high

# Storage Buckets
IMPORTS_BUCKET_NAME=cad-imports
EXPORTS_BUCKET_NAME=cad-exports

# Admin
FIRST_SUPERUSER=admin@buildingcompliance.com
FIRST_SUPERUSER_PASSWORD=changeme

# Jurisdiction-Specific (Hong Kong)
# HK_DATA_GOV_API_KEY is optional; DATA.GOV.HK endpoints are usually public
HK_DATA_GOV_API_KEY=public
# HK_GEOSPATIAL_TOKEN is required only for protected CSDI/LandsD geospatial APIs
HK_GEOSPATIAL_TOKEN=

# Jurisdiction-Specific (New Zealand)
# NZ_LINZ_API_KEY is optional for public datasets; required for rate-limited LINZ APIs
NZ_LINZ_API_KEY=public
